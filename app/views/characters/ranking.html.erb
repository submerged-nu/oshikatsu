<h1>キャラクターランキング</h1>
<% @ranking.each do |character| %>
  <div>
  <%= link_to character.name,posts_path(q: { name_cont: character.name }) %>
    <% if @most_liked_images[character.id] %>
      <%= image_tag(@most_liked_images[character.id].url, width: "150", height: "150") %>
    <% else %>
      <p>No image available</p>
    <% end %>
  </div>
<% end %>
